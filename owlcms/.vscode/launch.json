    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}",
            "vmArgs": "",
            "env": {"OWLCMS_INITIALDATA":"LARGEGROUP_DEMO", "OWLCMS_RESETMODE": "true"}
        },
        {
            "type": "java",
            "name": "Launch Main",
            "request": "launch",
            "mainClass": "app.owlcms.Main",
            "projectName": "owlcms",
            "vmArgs": "-Dvaadin.frontend.hotdeploy=true",
            "env": {"OWLCMS_INITIALDATA":"LARGEGROUP_DEMO", "OWLCMS_RESETMODE": "true"},
            "shortenCommandLine": "none"
        },
        {
            "type": "java",
            "name": "Launch Simulation",
            "request": "launch",
            "mainClass": "app.owlcms.Simulation",
            "projectName": "owlcms"
        },
        {
            "type": "java",
            "name": "Launch Speakers",
            "request": "launch",
            "mainClass": "app.owlcms.sound.Speakers",
            "projectName": "owlcms"
        }
    ]
}